{
  "name": "datapm-client-lib",
  "version": "0.32.39",
  "description": "DataPM Client library, which includes all logic used by the the command line and server to generate, manage, and fetch packages. ",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/big-armor/datapm.git",
    "directory": "client-lib"
  },
  "engines": {
    "node": ">=16.14.2",
    "npm": ">=8.1.2"
  },
  "scripts": {
    "prebuild": "cd ../lib && npm run build && cd ../client-lib && npx gulp prebuild && npm run codegen",
    "build": "tsc",
    "clean": "npx gulp clean",
    "postbuild": "npx gulp postbuild",
    "pretest": "npm run codegen",
    "precodegen": "cd ../lib && npm run codegen",
    "codegen": "npx @graphql-codegen/cli --config codegen.yml",
    "test": "CREDENTIALS_SECRET_KEY=test mocha --slow 3000 --timeout 60000 -r ts-node/register test/**/*.ts src/**/*.spec.ts",
    "test:unit": "CREDENTIALS_SECRET_KEY=test mocha --slow 3000 --timeout 60000 -r ts-node/register src/**/*.spec.ts",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "npx eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "eslint:fix": "npx eslint --fix \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "files": [
    "src/**/*"
  ],
