{
    "name": "datapm-registry",
    "version": "0.6.215",
    "description": "datapm is the easiest way to discover, obtain, manage, and publish data sets. This server acts as a registry (not a repository) of catalogs of packages. Features of datapm-registry include CRUD operations on catalogs and packages, and authorization for each operation. ",
    "main": "backend/dist/index.js",
    "typings": "backend/dist/index.d.ts",
    "module": "backend/dist/index.js",
    "scripts": {
        "clean": "rm -rf node_modules dist lib/node_modules lib/dist frontend/dist frontend/node_modules backend/dist backend/node_modules frontend/node_modules docs/website/node_modules docs/website/build",
        "build": "gulp",
        "build:parallel": "gulp buildParallel",
        "dev:frontend": "cd frontend && npm start",
        "dev:backend": "cd backend && npm start",
        "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
        "start": "TYPEORM_IS_DIST=true docker-compose -f docker/docker-compose-local-build.yml up",
        "start:from-docker-hub": "TYPEORM_IS_DIST=true DATAPM_VERSION=latest docker-compose -f docker/docker-compose.yml up",
        "bump-version": "gulp bumpVersion",
        "tag": "gulp gitPushTag",
        "deploy": "gulp deployAssets",
        "lint": "eslint . && prettier -c  --loglevel=warn .",
        "lint:fix": "eslint --fix . && prettier -w .",
        "test": "mocha --slow 3000 --timeout 60000 -r ts-node/register test/**/*.ts",
        "test:snyk": "snyk test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/big-armor/datapm-registery.git"
    },
    "author": "Travis Collins",
    "license": "CC-BY-NC-ND-4.0",
    "bugs": {
        "url": "https://github.com/big-armor/datapm-registry/issues"
    },
    "homepage": "https://github.com/big-armor/datapm-registry#readme",
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.19.0",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "chai": "^4.2.0",
        "concurrently": "^5.3.0",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-standard": "^16.0.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.1.0",
        "git-format-staged": "^2.1.0",
        "gulp": "^4.0.2",
        "husky": "^4.3.0",
        "maildev": "^1.1.0",
        "merge-stream": "^2.0.0",
        "mocha": "^8.2.1",
        "pidtree": "^0.5.0",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.1.0",
        "snyk": "^1.413.2",
        "testcontainers": "^6.4.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.5",
        "why-is-node-running": "^2.2.0"
    },
    "dependencies": {
        "through2": "^4.0.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
