{
    "name": "datapm-client",
    "version": "0.8.31",
    "description": "The client for manipulating data",
    "scripts": {
        "clean": "gulp clean",
        "prebuild": "npm run codegen",
        "build": "tsc",
        "start": "CREDENTIALS_SECRET_KEY=test node --trace-warnings --inspect -r ts-node/register src/main.ts",
        "start:test-data-server": "node -r ts-node/register test/test-data-server.ts",
        "start-registry": "DATAPM_VERSION=latest docker-compose -f node_modules/datapm-lib/docker-compose.yml up",
        "codegen": "graphql-codegen --config codegen.yml && gulp postCodegen",
        "package:macos": "gulp buildMacOS",
        "package:linux:rpm": "gulp buildLinuxRPM",
        "package:linux:apt": "gulp buildLinuxAPT",
        "package:win": "gulp buildWindows",
        "pretest": "npm run codegen",
        "test": "CREDENTIALS_SECRET_KEY=test mocha --slow 3000 --timeout 60000 -r ts-node/register test/integration/*.ts",
        "test:debug": "CREDENTIALS_SECRET_KEY=test npx mocha --slow 3000 --timeout 999999 -r ts-node/register",
        "test:full": "CREDENTIALS_SECRET_KEY=test mocha --slow 3000 --timeout 60000 -r ts-node/register test/**/*.ts",
        "lint": "npm run eslint",
        "lint:fix": "npm run eslint:fix",
        "eslint": "npx eslint \"src/**/*.ts\" \"test/**/*.ts\"",
        "eslint:fix": "npx eslint --fix \"src/**/*.ts\" \"test/**/*.ts\""
    },
    "main": "dist/src/main.js",
    "bin": {
        "datapm": "dist/src/main.js"
    },
    "files": [
        "dist/**/*"
    ],
    "preferGlobal": true,
    "pkg": {
        "assets": [
            "./node_modules/@types",
            "node_modules/mmmagic/build/Release/magic.node",
            "./node_modules/@apollo/client",
            "./node_modules/typescript/lib/*.d.ts",
            "src/**/*.ts",
            "assets/**",
            "./tsconfig.json"
        ]
    },
    "author": "Travis Collins",
    "license": "CC-BY-NC-ND-4.0",
    "devDependencies": {
        "@graphql-codegen/add": "^1.17.7",
        "@graphql-codegen/cli": "^1.17.8",
        "@graphql-codegen/introspection": "^1.17.8",
