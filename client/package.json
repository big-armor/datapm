        "@types/faker": "^5.1.5",
        "@types/follow-redirects": "^1.13.0",
        "@types/glob": "^7.1.3",
        "@types/glob-parent": "^5.1.0",
        "@types/graphql-fields": "^1.3.2",
        "@types/graphql-upload": "^8.0.3",
        "@types/is-glob": "^4.0.1",
        "@types/is-number": "^7.0.0",
        "@types/json-schema": "^7.0.8",
        "@types/knex": "^0.16.1",
        "@types/mime-types": "^2.1.0",
        "@types/mocha": "^8.0.3",
        "@types/mysql": "^2.15.15",
