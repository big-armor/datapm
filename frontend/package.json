{
    "name": "datapm-registry-frontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --aot --proxy-config proxy.conf.json ",
        "start:test": "ng serve --aot --proxy-config proxy.test.conf.json ",
        "prebuild": "rm -rf dist; npm run codegen",
        "build": "NODE_OPTIONS=--max_old_space_size=4096 npx ng build --prod",
        "test": "ng test",
        "test:ci": "ng test --watch false  --browsers=ChromeHeadless",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "codegen": "graphql-codegen --config codegen.yml"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.0.11",
        "@angular/cdk": "~10.2.0-rc.0-sha-7ec514be9",
