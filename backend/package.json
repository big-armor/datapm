{
	"name": "datapm-registry-backend",
	"description": "The datapm-registry server backend",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"module": "dist/index.js",
	"scripts": {
		"prebuild": "rm -rf dist; npm run codegen",
		"build": "tsc",
		"postbuild": "gulp",
		"start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
		"start:server": "cd dist && TYPEORM_IS_DIST=true  node index.js",
		"test": "mocha -r ts-node/register src/**/*.spec.ts",
		"codegen": "graphql-codegen --config codegen.yml"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/big-armor/datapm-registery.git"
	},
	"author": "Travis Collins",
	"license": "CC-BY-NC-ND-4.0",
	"bugs": {
		"url": "https://github.com/big-armor/datapm-registry/issues"
	},
	"mocha": {
		"timeout": "300000",
		"require": [
			"ts-node/register",
			"source-map-support/register"
		],
		"extension": "ts"
	},
	"homepage": "https://github.com/big-armor/datapm-registry#readme",
	"dependencies": {
		"@google-cloud/secret-manager": "^3.1.0",
		"@google-cloud/storage": "^5.1.1",
		"@sendgrid/helpers": "^7.2.0",
		"@sendgrid/mail": "^7.2.1",
		"ajv": "^6.12.3",
		"apollo-server": "^2.11.0",
		"apollo-server-constraint-directive": "^1.2.5",
		"atob": "^2.1.2",
		"dataloader": "^2.0.0",
		"datapm-lib": "^0.3.3",
		"express-enforces-ssl": "^1.1.0",
		"express-http-proxy": "^1.6.0",
		"express-jwt": "^6.0.0",
		"graphql": "^14.6.0",
		"graphql-fields": "^2.0.3",
		"graphql-scalars": "^1.2.2",
		"graphql-type-json": "^0.3.2",
		"gulp": "^4.0.2",
		"helmet": "^3.23.3",
		"jsonwebtoken": "^8.5.1",
		"jwks-rsa": "^1.8.1",
		"mixpanel": "^0.11.0",
		"mkdirp": "^1.0.4",
		"moment": "^2.27.0",
		"pg": "^8.3.0",
		"pg-god": "^1.0.5",
		"reflect-metadata": "^0.1.13",
		"semver": "^7.3.2",
		"typeorm": "0.2.25",
		"uuid": "^8.2.0"
	},
	"devDependencies": {
		"@graphql-codegen/add": "^1.13.1",
		"@graphql-codegen/cli": "^1.13.1",
		"@graphql-codegen/introspection": "^1.17.8",
		"@graphql-codegen/typescript": "^1.17.8",
		"@graphql-codegen/typescript-operations": "^1.13.1",
		"@graphql-codegen/typescript-resolvers": "^1.17.8",
		"@types/atob": "^2.1.2",
		"@types/chai": "^4.2.13",
		"@types/expect": "^24.3.0",
		"@types/express-enforces-ssl": "^1.1.1",
		"@types/express-http-proxy": "^1.6.1",
		"@types/graphql-fields": "^1.3.2",
		"@types/gulp": "^4.0.6",
		"@types/gulp-install": "^0.6.31",
		"@types/helmet": "0.0.47",
		"@types/json-schema": "^7.0.5",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/mixpanel": "^2.14.2",
		"@types/mkdirp": "^1.0.1",
		"@types/mocha": "^8.0.3",
		"@types/node": "^14.11.2",
		"@types/semver": "^7.3.1",
		"@types/uuid": "^8.0.0",
		"chai": "^4.2.0",
		"del": "^6.0.0",
		"husky": "^4.3.0",
		"pretty-quick": "^3.0.2",
		"mocha": "^8.1.3",
		"nodemon": "^2.0.4",
		"source-map-support": "^0.5.16",
		"ts-loader": "^6.2.2",
		"ts-node": "^8.8.1",
		"tsc-watch": "^4.2.3",
		"tslint": "^6.1.2",
		"typescript": "^3.8.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
