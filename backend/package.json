{
    "name": "datapm-registry-backend",
    "description": "The datapm-registry server backend.",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist/index.js",
    "scripts": {
        "prebuild": "rm -rf dist; npm run codegen",
        "build": "tsc",
        "postbuild": "gulp",
        "prestart": "npm run codegen && docker-compose -f ../docker/docker-compose-local-dev.yml  up -d ",
        "start": "TS_NODE_FILES=true . ./env.sh && nodemon --watch 'src' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
        "prestart-nowatch": "npm run codegen",
        "start-nowatch": ". ./env.sh && ts-node src/index.ts",
        "start:server": ". ./env.sh && cd dist && TYPEORM_IS_DIST=true  node index.js",
        "pretest": "npm run codegen",
        "test": "mocha -r ts-node/register --slow 2000 test/integration/*.ts src/**/*.spec.ts",
        "test:debug": "npm run build && npx mocha --slow 3000 --timeout 999999 -r ts-node/register",
        "codegen": "graphql-codegen --config codegen.yml && graphql-codegen --config codegen-test.yml"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/big-armor/datapm-registery.git"
    },
    "author": "Travis Collins",
    "license": "CC-BY-NC-ND-4.0",
    "bugs": {
        "url": "https://github.com/big-armor/datapm-registry/issues"
    },
    "homepage": "https://github.com/big-armor/datapm-registry#readme",
