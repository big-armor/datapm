{
    "name": "datapm-lib",
    "version": "0.32.45",
    "description": "Definitions and utilities for managing datapm package files.",
    "main": "src/main.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/big-armor/datapm.git",
        "directory": "lib"
    },
    "engines": {
        "node": ">=16.14.2",
        "npm": ">=8.1.2"
    },
    "scripts": {
        "lint": "tslint -p tsconfig.json",
        "pretty-quick": "pretty-quick --pattern \"(src|test)/**/*.ts\"",
        "clean": "gulp clean",
        "codegen": "gulp codegen",
        "prebuild": "npm run clean && npm run codegen",
        "build": "tsc",
        "postbuild": "gulp",
        "pretest": "npm run codegen",
        "test": "gqlint schema.gql && mocha --slow 3000 --timeout 60000 -r ts-node/register test/*.ts"
    },
    "files": [
        "**/*"
    ],
    "author": "Travis Collins",
    "license": "https://datapm.io/docs/license",
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/json-schema": "^7.0.5",
        "@types/mocha": "^8.0.4",
        "@types/node": "^16.11.22",
        "@types/semver": "^7.3.9",
        "chai": "^4.2.0",
        "gqlint": "^1.8.0",
        "gulp": "^4.0.2",
        "husky": "^4.3.0",
        "mocha": "^10.1.0",
        "prettier": "^2.0.5",
        "pretty-quick": "^3.1.0",
        "ts-lint": "^4.5.1",
        "ts-node": "^9.0.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "ajv": "^6.12.6",
        "base62": "^2.0.1",
        "cross-fetch": "^3.1.5",
        "fast-deep-equal": "^3.1.3",
        "semver": "^7.3.2"
    }
}
